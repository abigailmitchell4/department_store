<div class='container'>
  <h1>Click a Department to View Items</h1>
</div>
<div class='container'>
  <ul>
    <% @departments.each do |d| %>
      <li><%= link_to d.name, department_path(d)%></li>
    <% end %>
  </ul>
</div>
<div class='container'>
  <span><%= link_to 'Create a New Department', new_department_path %></span>
</div>
